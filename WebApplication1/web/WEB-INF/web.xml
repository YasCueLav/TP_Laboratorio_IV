<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>Servlets.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarClienteServlet</servlet-name>
        <servlet-class>Servlets.AltaClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarPuestoServlet</servlet-name>
        <servlet-class>Servlets.AltaPuestoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarAlquilerServlet</servlet-name>
        <servlet-class>Servlets.AltaAlquilerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarAlquilerServlet</servlet-name>
        <servlet-class>Servlets.ModificarAlquilerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarClienteServlet</servlet-name>
        <url-pattern>/RegistrarClienteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarPuestoServlet</servlet-name>
        <url-pattern>/RegistrarPuestoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarAlquilerServlet</servlet-name>
        <url-pattern>/RegistrarAlquilerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarAlquilerServlet</servlet-name>
        <url-pattern>/ModificarAlquilerServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>RevicionInicio.jsp</welcome-file>
    </welcome-file-list>
</web-app>
